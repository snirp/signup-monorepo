{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ES5",
    "lib": [
      "ES2015"
    ],
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["./**/*.ts", "../server/dist/*"],
  "exclude": ["node_modules", "../../node_modules", "./dist/**/*"]
}